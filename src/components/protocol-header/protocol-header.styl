.protocolHeader
  margin 0
  padding 10px 15px 20px
  font-size 1.15rem
  text-align center
  &__top
    position relative
  &__caption
    line-height 1
    font-size 1.5rem
    color #606065
  &__goBack
    position absolute
    left 0
    top 50%
    transform translateY(-50%)
    line-height 1
    padding-left 5px
  &__stepTitle
    padding-top 10px
    line-height 1
    text-align center
    font-size 1.35rem
    color #918e9c
