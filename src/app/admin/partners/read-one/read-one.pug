      table
        thead
          th Адрес
          th Номер
          th Успех
          th Оплата
        tbody
          tr
            td address
            td 000000000
            td -
            td
              button.button Оплатить
          tr
            td address1
            td 000000000
            td +
            td Оплачено
          tr
            td address2
            td 000000000
            td -
            td
              button.button Оплатить
