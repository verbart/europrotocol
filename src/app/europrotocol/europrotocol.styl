.europrotocol
  height 100%
  min-height 100%
  background #fffcef
  &__loader
    position fixed
    top 50%
    left 50%
    width 100%
    transform translate(-50%, -50%)
    padding 15px
    text-align center
    color #888
    font-size 1rem
  &__footer
    position fixed
    bottom 0
    left 0
    right 0
    z-index 800
    box-shadow 0 0 1px 1px #999
    background rgba(#fff, .8)
